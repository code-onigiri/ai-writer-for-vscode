{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-ext"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/vscode-ext/dist/**/*.js"
      ],
  "preLaunchTask": "build vscode extension"
    }
  ]
}